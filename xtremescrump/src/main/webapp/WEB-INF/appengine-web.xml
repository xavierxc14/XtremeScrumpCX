<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>${appengine.app.appId}</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>
    <sessions-enabled>true</sessions-enabled>
    <static-files>
        <include path="/favicon.ico" />
        <include path="/resources/**" />
        <include path="/css/style.css" />
        <include path="/css/general.css" />
        <include path="/css/iconic.css" />
        <include path="/js/prefix-free.js" />
        
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="cloudsql.url" value="${cloudsql.url}"/>
        <property name="cloudsql.url.dev" value="${cloudsql.url.dev}"/>
    </system-properties>
</appengine-web-app>
